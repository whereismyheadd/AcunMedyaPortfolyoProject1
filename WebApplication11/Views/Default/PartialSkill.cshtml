@model List<WebApplication11.Models.TblSkill>
<section id="skills" class="skills section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Skills</h2>
        <p>I have a good foundation in problem solving and algorithms. I adapt quickly to new technologies. I am a software developer who is always open to learning and improving.</p>
    </div><!-- End Section Title -->

    <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row skills-content skills-animation">

            <div class="col-lg-6">
                @foreach (var item in Model)
                {
                    <div class="progress">
                        <span class="skill">
                            <span>@item.SkillName  @item.Degree</span>
                            <div class="progress-bar-wrap">
                                <div class="progress-bar" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                    </div><!-- End Skills Item -->

                   





                }

            </div>

    </div>

</section><!-- /Skills Section -->
