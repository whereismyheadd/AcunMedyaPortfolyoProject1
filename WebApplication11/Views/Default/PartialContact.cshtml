@model List<WebApplication11.Models.TblContact>
<div class="col-lg-4">
    <div class="section-title" data-aos="fade-up" data-aos-delay="100">
        <h2>Contact</h2>
        <p>Your feedback is important to us. Contact us anytime, and we’ll respond promptly.</p>

    </div><!-- End Contact Section Title -->


    <div>
        <h3>Address</h3>
        <p> @Model[0].Adress</p>



    </div>
</div><!-- End Info Item -->

    <div class="info-item d-flex" data-aos="fade-up" data-aos-delay="300">
        <i class="bi bi-telephone flex-shrink-0"></i>
        <div>
            <h3>Phone Number </h3>
            <p>@Model[0].Phone</p>
        </div>
    </div><!-- End Info Item -->

    <div class="info-item d-flex" data-aos="fade-up" data-aos-delay="400">
        <i class="bi bi-envelope flex-shrink-0"></i>
        <div>
            <h3>Email</h3>
            <p>@Model[0].Email</p>
        </div>
    </div><!-- End Info Item -->

</div>

